@model IEnumerable<_420Project.Models.StudentPlan>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutPanel.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Course.Number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Program.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Semester.Season)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Student.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EnrollmentId)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Course.Number)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Program.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Semester.Season)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Student.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EnrollmentId)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.StudentPlanId }) |
            @Html.ActionLink("Details", "Details", new { id=item.StudentPlanId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.StudentPlanId })
        </td>
    </tr>
}

</table>
