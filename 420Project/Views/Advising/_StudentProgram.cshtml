@{ 
    Layout = null;
}
<script>
    // Advising Tabs Navigation
    $(document).ready(function(){
        var data = {id: @Session["AdvisingStudentId"]};

        $('#advising-tab-compliance').click(function () {
            $.ajax({
                type: "GET",
                data: data,
                url: "@(Url.Action("_StudentCompliance", "Advising"))",
                success: function (data) {
                    $('#advising-student-details-tabs').html(data);
                },
            error: function (data) {
                alert('error');

            }
        })
        })

        $('#advising-tab-plan').click(function () {
            $.ajax({
                type: "GET",
                data: data,
                url: "@(Url.Action("_StudentPlan", "Advising"))",
                success: function (data) {
                    $('#advising-student-details-tabs').html(data);
                },
            error: function (data) {
                alert('error');

            }
        })
        })

        $('#advising-tab-petition').click(function () {
            $.ajax({
                type: "GET",
                data: data,
                url: "@(Url.Action("_StudentPetition", "Advising"))",
                success: function (data) {
                    $('#advising-student-details-tabs').html(data);
                },
            error: function (data) {
                alert('error');

            }
        })
    })

    })


</script>

<div class="col-xs-12">
    <ul id="advising-tabs" class="nav nav-tabs">
        <li role="presentation"><a id="advising-tab-compliance" href="#">Compliance</a></li>
        <li role="presentation"  class="active advising-tab-active"><a id="advising-tab-program" href="#">Program</a></li>
        <li role="presentation"><a id="advising-tab-plan" href="#">Plan</a></li>
        <li role="presentation"><a id="advising-tab-petition" href="#">Petition</a></li>
    </ul>
    <br />
</div>

<div class="col-xs-12">
    
    <table id="advising-program-table" class="table table-striped table-bordered" data-toggle="table">
        <thead>
            <tr>
                <th>Program</th>
                <th>Semesters</th>
                <th>Hours</th>
                <th>GPA</th>
                <th></th>
            </tr>
        </thead>
        <tbody id="advising-program-body">
            <tr class="advising-program-row">
                <td>BSN - LD</td>
                <td>4</td>
                <td>50</td>
                <td class="text-center">3.4</td>
                <td class="text-center"><a href="#" class="glyphicon glyphicon-list-alt"></a></td>
            </tr>
            <tr class="advising-program-row">
                <td>BSN - UD</td>
                <td>3</td>
                <td>40</td>
                <td class="text-center">3.2</td>
                <td class="text-center"><a href="#" class="glyphicon glyphicon-list-alt"></a></td>
            </tr>
           
        </tbody>
    </table>
</div>