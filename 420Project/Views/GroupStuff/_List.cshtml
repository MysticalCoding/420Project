@{ 
    Layout = null;
}

<div class="panel-body">
    <div class="row">
        <div class="col-xs-12">
            <div class="col-xs-12">
                <div class="form-group"></div>

            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <div class="col-xs-6">
                <br />
                

                <div class="form-group">
                    <div class="col-md-12 text-left"><b>Year</b></div>
                    <div class="col-md-12">
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Freshman</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Sophmore</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Junior</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Senior</label>
                        </span>
                    </div>
                </div>

            </div>

            <div class="col-xs-6">
                <br />

                <div class="form-group">
                    <div class="col-md-12 text-left"><b>Program</b></div>
                    <div class="col-md-12">
                        <span class="checkbox">
                            <label><input type="checkbox" value="">BSN - LD</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">BSN - UD</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Master</label>
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-xs-12">
            <div class="col-xs-6">
                <br />

                <div class="form-group">

                    <div class="col-md-12 text-left"><b>Advisor</b></div>
                    <div class="col-md-12">
                        <div class="dropdown">
                            <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown">
                                All
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu">
                                <li><a href="#">All</a></li>
                                <li><a href="#">Advisor 1</a></li>
                                <li><a href="#">Advisor 2</a></li>
                            </ul>
                        </div>
                    </div>

                    <br />

                    <div class="col-md-12 text-left"><b>Campus</b></div>
                    <div class="col-md-12">
                        <div class="dropdown">
                            <button class="btn btn-default dropdown-toggle" type="button" data-toggle="dropdown">
                                All
                                <span class="caret"></span>
                            </button>
                            <ul class="dropdown-menu">
                                <li><a href="#">All</a></li>
                                <li><a href="#">Louisville</a></li>
                                <li><a href="#">Other</a></li>
                            </ul>
                        </div>
                    </div>

                </div>

                <div class="form-group">
                    <div class="col-xs-12">
                        <div class="btn btn-primary btn-lg" id="group-filters-apply"><span class="glyphicon glyphicon-filter"></span> Apply Filter</div>
                    </div>

                </div>

            </div>

            <div class="col-xs-6">
                <br />

                <div class="form-group">
                    <div class="col-md-12 text-left"><b>Status</b></div>
                    <div class="col-md-12">
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Unenrolled</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Active</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Inactive</label>
                        </span>
                        <span class="checkbox">
                            <label><input type="checkbox" value="">Graduated</label>
                        </span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
