<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>

    <link href="~/Content/jquery-ui.min.css" rel="stylesheet" />

    @RenderSection("css", required: false)

    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    <script src="~/Scripts/jquery-ui.min.js"></script>
 
    @RenderSection("scripts", required: false)

    <!-- Bootstrap table -->
    <link href="~/Content/bootstrap-table.min.css" rel="stylesheet" />
    <script src="~/Scripts/bootstrap-table.min.js"></script>
    <script src="~/Scripts/bootstrap-table-en-US.js"></script>
    <script src="~/Scripts/bootstrap-table-toolbar.min.js"></script>

    <link href="~/Content/Layout.css" rel="stylesheet" />



</head>
<body>
    <div id="container" class="container-fluid body-content">
        <div id="navbar" class="row">
            <div class="col-xs-2">
                <a href="/Dashboard">
                    <img id="logo" src="~/Content/Layout/logo-nursing_fullcolor.png" class="col-xs-12" />
                </a>
            </div>
            <div class="col-xs-8">
                <ul id="navigation" class="nav nav-pills">
                    <li id="nav-home" role="presentation">
                        <a href="/Dashboard/Index">HOME</a>
                </li>
                <li id="nav-news" role="presentation"><a href="/Dashboard/Development">NEWS</a></li>
                <li id="nav-reports" role="presentation"><a href="/Reports/Index">REPORTS</a></li>
                <li id="nav-progress" role="presentation">
                    <a href="/Progress/Index">PROGRESS</a>
              </li>
            <li id="nav-advising" role="presentation"><a href="/Dashboard/Development">ADVISING</a></li>
            <li id="nav-account" role="presentation"><a href="/Dashboard/Development">ACCOUNT</a></li>
        </ul>

    </div>
</div>

<div class="row" id="content-row">
    <div id="panel-content" class="panel panel-default">
        <div id="content" class="panel-body">
            @RenderBody()
        </div>
    </div>
</div>
<hr />
<footer>
    <p>&copy; @DateTime.Now.Year - University of Louisville</p>
</footer>
</div>


</body>
</html>